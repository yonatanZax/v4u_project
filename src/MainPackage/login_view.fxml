<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainPackage.ControllerLogin">
   <children>
      <Region prefHeight="34.0" prefWidth="600.0" />
      <Label text="Welcome">
         <font>
            <Font size="14.0" />
         </font></Label>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
             <Label text="User name">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets></Label>
            <Region opacity="0.0" prefHeight="100.0" prefWidth="50.0" />
            <TextField fx:id="userName">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets></TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
             <Label text="Password" />
            <Region opacity="0.0" prefHeight="100.0" prefWidth="50.0" />
            <PasswordField fx:id="password" />
         </children>
      </HBox>
      <Label fx:id="info_lbl" text="Info:">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="readUser_btn" mnemonicParsing="false" onAction="#readUser" text="Read">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin></Button>
      <Button fx:id="createUser_btn" mnemonicParsing="false" onAction="#createUser" text="Create">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
      <Button fx:id="updateUser_btn" mnemonicParsing="false" onAction="#updateUser" text="Update" />
      <Button fx:id="deleteUser_btn" mnemonicParsing="false" onAction="#deleteUser" text="Delete" />

      <Label fx:id="error_lbl" alignment="CENTER" prefHeight="25.0" prefWidth="196.0" textAlignment="CENTER" textFill="RED" />
   </children>
</VBox>
